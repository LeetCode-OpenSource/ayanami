{
  "name": "ayanami",
  "version": "1.0.0-rc.7",
  "description": "A better way to react with state",
  "main": "./dist/index.js",
  "module": "./esm/index.js",
  "esnext": "./esnext/index.js",
  "types": "./esm/index.d.ts",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LeetCode-OpenSource/ayanami.git"
  },
  "bugs": {
    "url": "https://github.com/LeetCode-OpenSource/ayanami/issues"
  },
  "homepage": "https://github.com/LeetCode-OpenSource/ayanami#readme",
  "scripts": {
    "check_circular_dependencies": "madge esm/index.js --circular --warning",
    "demo": "parcel ./demo/index.html",
    "build": "npm-run-all -p build:es5 build:esm build:next",
    "build:es5": "shx rm -rf ./dist && tsc -p ./tsconfig.build.json",
    "build:esm": "shx rm -rf ./esm && tsc -p ./tsconfig.build.json -m esnext --outDir esm",
    "build:next": "shx rm -rf ./esnext && tsc -p ./tsconfig.build.json --target esnext --outDir esnext",
    "prettier": "prettier '@(src|demo)/**/*.@(ts|tsx|html|less)' --write",
    "lint": "yarn lint:eslint && yarn lint:tsc",
    "lint:eslint": "eslint . --ext .ts,.tsx --fix --max-warnings 0",
    "lint:tsc": "tsc -p ./tsconfig.json --noEmit",
    "test": "jest --collectCoverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "yarn lint:eslint",
      "git add"
    ],
    "*.{less,html}": [
      "prettier --write",
      "git add"
    ]
  },
  "keywords": [
    "React",
    "hooks",
    "Observables",
    "Observable",
    "model",
    "state",
    "Rx",
    "RxJS",
    "ReactiveX"
  ],
  "author": "LeetCode front-end team",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/jest": "^24.0.25",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.1.4",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-test-renderer": "^16.9.1",
    "@types/sinon": "^7.5.1",
    "@typescript-eslint/eslint-plugin": "^2.15.0",
    "@typescript-eslint/parser": "^2.15.0",
    "codecov": "^3.6.1",
    "eslint": "6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-react": "^7.17.0",
    "husky": "^4.0.1",
    "immer": "^5.1.0",
    "jest": "^24.9.0",
    "lint-staged": "^9.5.0",
    "lodash": "^4.17.15",
    "madge": "^3.6.0",
    "npm-run-all": "^4.1.5",
    "parcel": "^1.12.4",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-test-renderer": "^16.12.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.4",
    "shx": "^0.3.2",
    "sinon": "^8.0.4",
    "ts-jest": "^24.3.0",
    "tslib": "^1.10.0",
    "typescript": "^3.7.4"
  },
  "peerDependencies": {
    "immer": "^5.1.0",
    "react": "^16.8.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.4"
  },
  "dependencies": {
    "@asuka/di": "^0.3.4",
    "@emotion/core": "^10.0.27"
  }
}
