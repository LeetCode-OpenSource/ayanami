{
  "name": "ayanami",
  "version": "0.8.2",
  "description": "A better way to react with state",
  "main": "./dist/index.js",
  "module": "./esm/index.js",
  "esnext": "./esnext/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LeetCode-OpenSource/ayanami.git"
  },
  "bugs": {
    "url": "https://github.com/LeetCode-OpenSource/ayanami/issues"
  },
  "homepage": "https://github.com/LeetCode-OpenSource/ayanami#readme",
  "scripts": {
    "check_circular_dependencies": "madge esm/index.js --circular --warning",
    "demo": "parcel ./demo/index.html",
    "build": "npm-run-all -p build:es5 build:esm build:next",
    "build:es5": "shx rm -rf ./dist && tsc -p ./tsconfig.build.json",
    "build:esm": "shx rm -rf ./esm && tsc -p ./tsconfig.build.json -m esnext --outDir esm",
    "build:next": "shx rm -rf ./esnext && tsc -p ./tsconfig.build.json --target esnext --outDir esnext",
    "prettier": "prettier '@(src|demo)/**/*.@(ts|tsx|html|less)' --write",
    "lint": "tslint -p tsconfig.json --fix",
    "test": "jest --collectCoverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "tslint -p tsconfig.json --fix",
      "git add"
    ],
    "*.{less,html}": [
      "prettier --write",
      "git add"
    ]
  },
  "keywords": [
    "React",
    "hooks",
    "Observables",
    "Observable",
    "model",
    "state",
    "Rx",
    "RxJS",
    "ReactiveX"
  ],
  "author": "LeetCode front-end team",
  "license": "MIT",
  "dependencies": {
    "lodash": "^4.17.11",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.4.0",
    "shallowequal": "^1.1.0"
  },
  "devDependencies": {
    "@asuka/di": "^0.1.5",
    "@types/jest": "^24.0.11",
    "@types/lodash": "^4.14.123",
    "@types/react": "^16.8.10",
    "@types/react-dom": "^16.8.3",
    "@types/react-test-renderer": "^16.8.1",
    "@types/shallowequal": "^1.1.1",
    "codecov": "^3.2.0",
    "husky": "^1.3.1",
    "jest": "^24.5.0",
    "lint-staged": "^8.1.5",
    "madge": "^3.4.4",
    "npm-run-all": "^4.1.5",
    "parcel": "^1.12.3",
    "prettier": "^1.16.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-test-renderer": "^16.8.6",
    "shx": "^0.3.2",
    "ts-jest": "^24.0.1",
    "tslib": "^1.9.3",
    "tslint": "^5.14.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^4.0.0",
    "tslint-sonarts": "^1.9.0",
    "typescript": "^3.4.1"
  },
  "peerDependencies": {
    "@asuka/di": "^0.1.3",
    "react": "^16.8.6"
  }
}
